# Server Configuration
PORT=3000
NODE_ENV=development

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/order_engine

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Queue Configuration
QUEUE_CONCURRENCY=10
ORDERS_PER_MINUTE=100

# Logging
LOG_LEVEL=info

# Blockchain Configuration (Optional - for real devnet execution)
USE_REAL_BLOCKCHAIN=false
SOLANA_NETWORK=devnet
SOLANA_RPC_URL=https://api.devnet.solana.com
# SOLANA_PRIVATE_KEY=your_base58_encoded_private_key_here
# Note: For devnet testing, you can leave SOLANA_PRIVATE_KEY empty to use a generated test wallet
